        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Midstart">
        <activity android:name=".ChatBotActivity"></activity>
        <activity android:name=".CESresActivity" />
        <activity android:name=".CESActivity" />
        <activity android:name=".CardNews1" />
        <activity android:name=".CardNewsActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>